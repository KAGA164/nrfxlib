config RP_SERIALIZATION
	bool "Enable serialization library"
	help
	  Enable serialization library, with chosen transport layer

if RP_SERIALIZATION

choice
	prompt "Serialization transport"
	default RMPSG_TRANSPORT

config RMPSG_TRANSPORT
	bool "Serialization RPMsg transport"

endchoice

endif # RP_SERIALIZATION
